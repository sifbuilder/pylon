        width: {width} \n \
        input_channels: {input_channels} \n \
        buffer_size: {buffer_size} \n \
        batch_size: {batch_size} \n \
        input_shape: {input_shape} \n \
    ")

    if 1: # model
        gan = GAN(
            models_dir = args.models_dir,
            logs_dir = args.logs_dir,
            ckpt_dir = ckpt_dir,
            ckpt_prefix = ckpt_prefix,
            input_shape = input_shape,
